{
  "name": "crustdata",
  "displayName": "Crustdata",
  "description": "AI-powered data intelligence platform providing real-time company and people data via APIs and webhooks",
  "category": "marketing",
  "iconUrl": "https://cdn.jsdelivr.net/npm/simple-icons@v9/icons/databricks.svg",
  "port": 49248  ,
  "api": {
    "baseURL": "https://api.crustdata.com",
    "version": "v1",
    "documentation": "https://docs.crustdata.com/"
  },
  "auth": {
    "type": "api_key",
    "fields": [
      {
        "name": "api_key",
        "type": "password",
        "required": true,
        "description": "Crustdata API Key"
      }
    ]
  },
  "tools": [
    {
      "name": "enrich_person",
      "description": "Enriches person data by providing additional information based on query",
      "endpoint": "/screener/person/enrich",
      "method": "POST",
      "parameters": {
        "linkedin_profile_url": {
          "type": "string",
          "required": false,
          "description": "LinkedIn profile URL of the person"
        },
        "email": {
          "type": "string",
          "required": false,
          "description": "Email address of the person"
        },
        "full_name": {
          "type": "string",
          "required": false,
          "description": "Full name of the person"
        },
        "company_domain": {
          "type": "string",
          "required": false,
          "description": "Company domain where the person works"
        },
        "enrich_realtime": {
          "type": "boolean",
          "required": false,
          "description": "Whether to enrich in real-time",
          "default": false
        },
        "fields": {
          "type": "string",
          "required": false,
          "description": "Comma-separated list of fields to include in response"
        }
      }
    },
    {
      "name": "search_companies",
      "description": "Search and filter companies based on various criteria",
      "endpoint": "/screener/company",
      "method": "POST",
      "parameters": {
        "company_domain": {
          "type": "string",
          "required": false,
          "description": "Company domain to search for"
        },
        "company_name": {
          "type": "string",
          "required": false,
          "description": "Company name to search for"
        },
        "industry": {
          "type": "string",
          "required": false,
          "description": "Industry filter"
        },
        "location": {
          "type": "string",
          "required": false,
          "description": "Location filter"
        },
        "employee_count_min": {
          "type": "number",
          "required": false,
          "description": "Minimum employee count"
        },
        "employee_count_max": {
          "type": "number",
          "required": false,
          "description": "Maximum employee count"
        },
        "revenue_min": {
          "type": "number",
          "required": false,
          "description": "Minimum revenue"
        },
        "revenue_max": {
          "type": "number",
          "required": false,
          "description": "Maximum revenue"
        },
        "enrich_realtime": {
          "type": "boolean",
          "required": false,
          "description": "Whether to enrich in real-time",
          "default": false
        },
        "limit": {
          "type": "number",
          "required": false,
          "description": "Number of results to return",
          "default": 100
        }
      }
    },
    {
      "name": "enrich_company",
      "description": "Enrich company data with additional intelligence",
      "endpoint": "/screener/company/enrich",
      "method": "POST",
      "parameters": {
        "company_domain": {
          "type": "string",
          "required": true,
          "description": "Company domain to enrich"
        },
        "enrich_realtime": {
          "type": "boolean",
          "required": false,
          "description": "Whether to enrich in real-time",
          "default": false
        },
        "fields": {
          "type": "string",
          "required": false,
          "description": "Comma-separated list of fields to include in response"
        }
      }
    },
    {
      "name": "search_people",
      "description": "Search for people based on various criteria",
      "endpoint": "/screener/person",
      "method": "POST",
      "parameters": {
        "company_domain": {
          "type": "string",
          "required": false,
          "description": "Company domain filter"
        },
        "job_title": {
          "type": "string",
          "required": false,
          "description": "Job title filter"
        },
        "seniority_level": {
          "type": "string",
          "required": false,
          "description": "Seniority level filter"
        },
        "location": {
          "type": "string",
          "required": false,
          "description": "Location filter"
        },
        "department": {
          "type": "string",
          "required": false,
          "description": "Department filter"
        },
        "limit": {
          "type": "number",
          "required": false,
          "description": "Number of results to return",
          "default": 100
        }
      }
    }
  ]
}