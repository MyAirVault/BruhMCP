{
  "#coming_soon": "OneDrive documentation is not yet available in Composio v3 docs",
  "name": "onedrive",
  "displayName": "OneDrive",
  "description": "Microsoft OneDrive cloud storage service for file syncing, sharing, and collaboration",
  "category": "storage",
  "iconUrl": "https://cdn.jsdelivr.net/npm/simple-icons@v9/icons/microsoftonedrive.svg",
  "port": 49394  ,
  "api": {
    "baseURL": "https://graph.microsoft.com",
    "version": "v1.0",
    "documentation": "https://docs.microsoft.com/en-us/graph/api/resources/onedrive"
  },
  "auth": {
    "type": "oauth2",
    "fields": [
      {
        "name": "client_id",
        "type": "text",
        "required": true,
        "description": "Microsoft Azure App Client ID"
      },
      {
        "name": "client_secret",
        "type": "password",
        "required": true,
        "description": "Microsoft Azure App Client Secret"
      }
    ],
    "oauth": {
      "redirect_uri": "https://backend.composio.dev/api/v1/auth-apps/add",
      "scopes": ["Files.ReadWrite", "Files.ReadWrite.All"],
      "authorization_url": "https://login.microsoftonline.com/common/oauth2/v2.0/authorize",
      "token_url": "https://login.microsoftonline.com/common/oauth2/v2.0/token"
    }
  },
  "tools": []
}