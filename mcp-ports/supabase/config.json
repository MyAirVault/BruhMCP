{
  "name": "supabase",
  "displayName": "Supabase",
  "description": "Supabase is an open-source backend-as-a-service providing a Postgres database, authentication, storage, and real-time subscription APIs",
  "category": "backend-as-a-service",
  "iconUrl": "https://cdn.jsdelivr.net/npm/simple-icons@v9/icons/supabase.svg",
  "port": 49473  ,
  "api": {
    "baseURL": "https://api.supabase.com",
    "version": "v1",
    "documentation": "https://supabase.com/docs/reference/api"
  },
  "auth": {
    "type": "oauth2",
    "fields": [
      {
        "name": "client_id",
        "type": "text",
        "required": true,
        "description": "Supabase OAuth App Client ID"
      },
      {
        "name": "client_secret",
        "type": "password",
        "required": true,
        "description": "Supabase OAuth App Client Secret"
      }
    ],
    "oauth": {
      "redirect_uri": "https://backend.composio.dev/api/v1/auth-apps/add",
      "scopes": ["all"],
      "authorization_url": "https://api.supabase.com/v1/authorize",
      "token_url": "https://api.supabase.com/v1/oauth/token"
    }
  },
  "tools": [
    {
      "name": "execute_sql_query",
      "description": "Execute SQL queries on Supabase database",
      "endpoint": "/v1/projects/{project_ref}/database/query",
      "method": "POST",
      "parameters": {
        "project_ref": {
          "type": "string",
          "required": true,
          "description": "Supabase project reference ID"
        },
        "query": {
          "type": "string",
          "required": true,
          "description": "SQL query to execute"
        },
        "params": {
          "type": "array",
          "required": false,
          "description": "Query parameters for prepared statements"
        }
      }
    },
    {
      "name": "create_organization",
      "description": "Create a new organization",
      "endpoint": "/v1/organizations",
      "method": "POST",
      "parameters": {
        "name": {
          "type": "string",
          "required": true,
          "description": "Organization name"
        },
        "billing_email": {
          "type": "string",
          "required": false,
          "description": "Organization billing email"
        },
        "tier": {
          "type": "string",
          "required": false,
          "description": "Organization tier (free, pro, team, enterprise)"
        }
      }
    },
    {
      "name": "create_project",
      "description": "Create a new project",
      "endpoint": "/v1/projects",
      "method": "POST",
      "parameters": {
        "organization_id": {
          "type": "string",
          "required": true,
          "description": "Organization ID"
        },
        "name": {
          "type": "string",
          "required": true,
          "description": "Project name"
        },
        "database_password": {
          "type": "string",
          "required": true,
          "description": "Database password"
        },
        "region": {
          "type": "string",
          "required": false,
          "description": "Project region"
        },
        "plan": {
          "type": "string",
          "required": false,
          "description": "Project plan (free, pro)"
        }
      }
    },
    {
      "name": "list_projects",
      "description": "List all projects",
      "endpoint": "/v1/projects",
      "method": "GET",
      "parameters": {
        "organization_id": {
          "type": "string",
          "required": false,
          "description": "Filter by organization ID"
        }
      }
    },
    {
      "name": "manage_api_keys",
      "description": "Manage project API keys",
      "endpoint": "/v1/projects/{project_ref}/api-keys",
      "method": "GET",
      "parameters": {
        "project_ref": {
          "type": "string",
          "required": true,
          "description": "Supabase project reference ID"
        }
      }
    },
    {
      "name": "configure_webhook",
      "description": "Configure project webhooks",
      "endpoint": "/v1/projects/{project_ref}/database/webhooks",
      "method": "POST",
      "parameters": {
        "project_ref": {
          "type": "string",
          "required": true,
          "description": "Supabase project reference ID"
        },
        "name": {
          "type": "string",
          "required": true,
          "description": "Webhook name"
        },
        "events": {
          "type": "array",
          "required": true,
          "description": "Array of events to trigger webhook"
        },
        "url": {
          "type": "string",
          "required": true,
          "description": "Webhook URL endpoint"
        },
        "headers": {
          "type": "object",
          "required": false,
          "description": "Custom headers for webhook requests"
        }
      }
    },
    {
      "name": "enable_auth_provider",
      "description": "Enable third-party authentication provider",
      "endpoint": "/v1/projects/{project_ref}/config/auth",
      "method": "PATCH",
      "parameters": {
        "project_ref": {
          "type": "string",
          "required": true,
          "description": "Supabase project reference ID"
        },
        "provider": {
          "type": "string",
          "required": true,
          "description": "Authentication provider (google, github, apple, etc.)"
        },
        "enabled": {
          "type": "boolean",
          "required": true,
          "description": "Whether to enable the provider"
        },
        "client_id": {
          "type": "string",
          "required": false,
          "description": "Provider client ID"
        },
        "client_secret": {
          "type": "string",
          "required": false,
          "description": "Provider client secret"
        }
      }
    },
    {
      "name": "get_project_settings",
      "description": "Get project settings and configuration",
      "endpoint": "/v1/projects/{project_ref}/config",
      "method": "GET",
      "parameters": {
        "project_ref": {
          "type": "string",
          "required": true,
          "description": "Supabase project reference ID"
        }
      }
    }
  ]
}