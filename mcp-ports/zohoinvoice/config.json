{
  "#coming_soon": "Documentation for Zoho Invoice is not yet available in the Composio docs",
  "name": "zohoinvoice",
  "displayName": "Zoho Invoice",
  "description": "Zoho Invoice is an online invoicing software that helps you craft professional invoices, send payment reminders, and get paid faster",
  "category": "finance",
  "iconUrl": "https://cdn.jsdelivr.net/npm/simple-icons@v9/icons/zoho.svg",
  "port": 49529  ,
  "api": {
    "baseURL": "https://api.composio.dev",
    "version": "v1",
    "documentation": "https://v3.docs.composio.dev/tools/zohoinvoice"
  },
  "auth": {
    "type": "oauth2",
    "fields": [
      {
        "name": "client_id",
        "type": "string",
        "required": true,
        "description": "Zoho Invoice OAuth2 Client ID"
      },
      {
        "name": "client_secret",
        "type": "password",
        "required": true,
        "description": "Zoho Invoice OAuth2 Client Secret"
      },
      {
        "name": "oauth_redirect_uri",
        "type": "string",
        "required": false,
        "description": "OAuth2 Redirect URI",
        "default": "https://backend.composio.dev/api/v1/auth-apps/add"
      }
    ]
  },
  "tools": []
}