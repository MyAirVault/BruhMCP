{
  "name": "openai",
  "displayName": "OpenAI",
  "description": "AI platform providing GPT models and other AI capabilities for text generation, completion, and embeddings",
  "category": "ai",
  "iconUrl": "https://cdn.jsdelivr.net/npm/simple-icons@v9/icons/openai.svg",
  "port": 49396  ,
  "api": {
    "baseURL": "https://api.openai.com/v1",
    "version": "v1",
    "documentation": "https://platform.openai.com/docs/api-reference"
  },
  "auth": {
    "type": "api_key",
    "fields": [
      {
        "name": "api_key",
        "type": "password",
        "required": true,
        "description": "OpenAI API Key (starts with 'sk-')"
      }
    ]
  },
  "tools": [
    {
      "name": "create_completion",
      "description": "Create a text completion using GPT models",
      "endpoint": "/completions",
      "method": "POST",
      "parameters": {
        "model": {
          "type": "string",
          "required": true,
          "description": "Model to use for completion"
        },
        "prompt": {
          "type": "string",
          "required": true,
          "description": "The prompt to complete"
        },
        "max_tokens": {
          "type": "number",
          "required": false,
          "description": "Maximum tokens to generate"
        }
      }
    },
    {
      "name": "create_chat_completion",
      "description": "Create a chat completion using chat models",
      "endpoint": "/chat/completions",
      "method": "POST",
      "parameters": {
        "model": {
          "type": "string",
          "required": true,
          "description": "Model to use for chat completion"
        },
        "messages": {
          "type": "array",
          "required": true,
          "description": "Array of message objects"
        }
      }
    },
    {
      "name": "list_models",
      "description": "List available models",
      "endpoint": "/models",
      "method": "GET",
      "parameters": {}
    }
  ]
}