{
  "#coming_soon": "This service configuration is not yet fully implemented. Canva API is in limited access.",
  "name": "canva",
  "displayName": "Canva",
  "description": "Online design platform for creating graphics, presentations, and visual content",
  "category": "design",
  "iconUrl": "https://cdn.jsdelivr.net/npm/simple-icons@v9/icons/canva.svg",
  "port": 49228  ,
  "api": {
    "baseURL": "https://api.canva.com/rest/v1",
    "version": "v1",
    "documentation": "https://www.canva.dev/docs/connect/api-reference/"
  },
  "auth": {
    "type": "oauth2",
    "fields": [
      {
        "name": "client_id",
        "type": "text",
        "required": true,
        "description": "Canva Connect App Client ID"
      },
      {
        "name": "client_secret",
        "type": "password",
        "required": true,
        "description": "Canva Connect App Client Secret"
      }
    ],
    "oauth": {
      "redirect_uri": "https://backend.composio.dev/api/v1/auth-apps/add",
      "scopes": ["design:read", "design:write"],
      "authorization_url": "https://www.canva.com/api/oauth/authorize",
      "token_url": "https://api.canva.com/rest/v1/oauth/token"
    }
  },
  "tools": [
    {
      "name": "list_designs",
      "description": "List user's designs",
      "endpoint": "/designs",
      "method": "GET",
      "parameters": {
        "limit": {
          "type": "number",
          "required": false,
          "description": "Number of designs to return"
        }
      }
    },
    {
      "name": "get_design",
      "description": "Get details of a specific design",
      "endpoint": "/designs/{designId}",
      "method": "GET",
      "parameters": {
        "designId": {
          "type": "string",
          "required": true,
          "description": "Design ID"
        }
      }
    },
    {
      "name": "create_design",
      "description": "Create a new design",
      "endpoint": "/designs",
      "method": "POST",
      "parameters": {
        "design_type": {
          "type": "string",
          "required": true,
          "description": "Type of design to create"
        }
      }
    }
  ]
}