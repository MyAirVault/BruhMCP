{
  "name": "firecrawl",
  "displayName": "Firecrawl",
  "description": "Automate web crawling and data extraction, enabling organizations to gather content, index sites, and gain insights from online sources at scale",
  "category": "analytics",
  "iconUrl": "https://cdn.jsdelivr.net/npm/simple-icons@v9/icons/firefoxbrowser.svg",
  "port": 49283  ,
  "api": {
    "baseURL": "https://api.firecrawl.dev/v1",
    "version": "v1",
    "documentation": "https://v3.docs.composio.dev/tools/firecrawl"
  },
  "auth": {
    "type": "api_key",
    "fields": [
      {
        "name": "generic_api_key",
        "type": "password",
        "required": true,
        "description": "Firecrawl API Key"
      }
    ]
  },
  "tools": [
    {
      "name": "cancel_crawl_job",
      "description": "Cancel a running crawl job",
      "endpoint": "/crawl/{jobId}/cancel",
      "method": "DELETE",
      "parameters": {
        "jobId": {
          "type": "string",
          "required": true,
          "description": "Job ID to cancel"
        }
      }
    },
    {
      "name": "get_crawl_status",
      "description": "Get status of a crawl job",
      "endpoint": "/crawl/{jobId}",
      "method": "GET",
      "parameters": {
        "jobId": {
          "type": "string",
          "required": true,
          "description": "Job ID to get status for"
        }
      }
    },
    {
      "name": "map_urls",
      "description": "Map multiple URLs for crawling preparation",
      "endpoint": "/map",
      "method": "POST",
      "parameters": {
        "url": {
          "type": "string",
          "required": true,
          "description": "Base URL to map"
        },
        "search": {
          "type": "string",
          "required": false,
          "description": "Search pattern for URL mapping"
        },
        "ignoreSitemap": {
          "type": "boolean",
          "required": false,
          "description": "Whether to ignore sitemap during mapping"
        },
        "includeSubdomains": {
          "type": "boolean",
          "required": false,
          "description": "Whether to include subdomains"
        },
        "limit": {
          "type": "number",
          "required": false,
          "description": "Maximum number of URLs to map"
        }
      }
    },
    {
      "name": "start_crawl",
      "description": "Start a web crawl job",
      "endpoint": "/crawl",
      "method": "POST",
      "parameters": {
        "url": {
          "type": "string",
          "required": true,
          "description": "URL to start crawling from"
        },
        "crawlerOptions": {
          "type": "object",
          "required": false,
          "description": "Crawler configuration options"
        },
        "pageOptions": {
          "type": "object",
          "required": false,
          "description": "Page processing options"
        }
      }
    },
    {
      "name": "extract_structured_data",
      "description": "Extract structured data from web pages",
      "endpoint": "/extract",
      "method": "POST",
      "parameters": {
        "urls": {
          "type": "array",
          "required": true,
          "description": "Array of URLs to extract data from"
        },
        "schema": {
          "type": "object",
          "required": true,
          "description": "Schema definition for structured data extraction"
        },
        "prompt": {
          "type": "string",
          "required": false,
          "description": "Additional prompt for extraction guidance"
        }
      }
    },
    {
      "name": "scrape_url",
      "description": "Scrape content from a single URL",
      "endpoint": "/scrape",
      "method": "POST",
      "parameters": {
        "url": {
          "type": "string",
          "required": true,
          "description": "URL to scrape"
        },
        "formats": {
          "type": "array",
          "required": false,
          "description": "Output formats (markdown, html, rawHtml, links, screenshot)"
        },
        "includeTags": {
          "type": "array",
          "required": false,
          "description": "HTML tags to include in scraping"
        },
        "excludeTags": {
          "type": "array",
          "required": false,
          "description": "HTML tags to exclude from scraping"
        },
        "onlyMainContent": {
          "type": "boolean",
          "required": false,
          "description": "Whether to extract only main content"
        }
      }
    },
    {
      "name": "web_search",
      "description": "Perform web search and scrape results",
      "endpoint": "/search",
      "method": "POST",
      "parameters": {
        "query": {
          "type": "string",
          "required": true,
          "description": "Search query"
        },
        "pageOptions": {
          "type": "object",
          "required": false,
          "description": "Page processing options for search results"
        },
        "searchOptions": {
          "type": "object",
          "required": false,
          "description": "Search configuration options"
        }
      }
    }
  ]
}