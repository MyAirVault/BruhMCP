{
  "name": "browseai",
  "displayName": "Browse.ai",
  "description": "Turn any website into an API using advanced web automation and data extraction tools",
  "category": "analytics",
  "iconUrl": "https://cdn.jsdelivr.net/npm/simple-icons@v9/icons/browsersync.svg",
  "port": 49218   ,
  "api": {
    "baseURL": "https://api.browse.ai/v2",
    "version": "v2",
    "documentation": "https://v3.docs.composio.dev/tools/browseai"
  },
  "auth": {
    "type": "api_key",
    "fields": [
      {
        "name": "generic_api_key",
        "type": "password",
        "required": true,
        "description": "Browse.ai API Key"
      }
    ]
  },
  "tools": [
    {
      "name": "bulk_run_tasks",
      "description": "Run up to 50,000 tasks using a specified robot",
      "endpoint": "/robots/{robotId}/tasks/bulk",
      "method": "POST",
      "parameters": {
        "robotId": {
          "type": "string",
          "required": true,
          "description": "Robot ID to run tasks for"
        }
      }
    },
    {
      "name": "create_monitor",
      "description": "Create a monitor with custom schedules",
      "endpoint": "/monitors",
      "method": "POST",
      "parameters": {
        "robotId": {
          "type": "string",
          "required": true,
          "description": "Robot ID to monitor"
        },
        "schedule": {
          "type": "object",
          "required": true,
          "description": "Monitor schedule configuration"
        }
      }
    },
    {
      "name": "create_webhook",
      "description": "Create a webhook for notifications",
      "endpoint": "/webhooks",
      "method": "POST",
      "parameters": {
        "url": {
          "type": "string",
          "required": true,
          "description": "Webhook URL"
        },
        "events": {
          "type": "array",
          "required": true,
          "description": "Events to trigger webhook"
        }
      }
    },
    {
      "name": "delete_monitor",
      "description": "Delete a specific monitor",
      "endpoint": "/monitors/{monitorId}",
      "method": "DELETE",
      "parameters": {
        "monitorId": {
          "type": "string",
          "required": true,
          "description": "Monitor ID to delete"
        }
      }
    },
    {
      "name": "delete_task",
      "description": "Delete a specific task",
      "endpoint": "/tasks/{taskId}",
      "method": "DELETE",
      "parameters": {
        "taskId": {
          "type": "string",
          "required": true,
          "description": "Task ID to delete"
        }
      }
    },
    {
      "name": "get_robots_list",
      "description": "Get list of available robots",
      "endpoint": "/robots",
      "method": "GET",
      "parameters": {}
    },
    {
      "name": "get_robot_tasks",
      "description": "Get tasks for a specific robot",
      "endpoint": "/robots/{robotId}/tasks",
      "method": "GET",
      "parameters": {
        "robotId": {
          "type": "string",
          "required": true,
          "description": "Robot ID to get tasks for"
        }
      }
    },
    {
      "name": "get_task_details",
      "description": "Get details for a specific task",
      "endpoint": "/tasks/{taskId}",
      "method": "GET",
      "parameters": {
        "taskId": {
          "type": "string",
          "required": true,
          "description": "Task ID to get details for"
        }
      }
    },
    {
      "name": "run_robot",
      "description": "Run a robot to perform web automation",
      "endpoint": "/robots/{robotId}/tasks",
      "method": "POST",
      "parameters": {
        "robotId": {
          "type": "string",
          "required": true,
          "description": "Robot ID to run"
        },
        "inputParameters": {
          "type": "object",
          "required": false,
          "description": "Input parameters for the robot"
        }
      }
    }
  ]
}