{
  "name": "recallai",
  "displayName": "Recall.ai",
  "description": "The universal API for meeting bots & conversation data, providing real-time transcription and recording capabilities",
  "category": "finance",
  "iconUrl": "https://cdn.jsdelivr.net/npm/simple-icons@v9/icons/microphone.svg",
  "port": 49423  ,
  "api": {
    "baseURL": "https://api.composio.dev",
    "version": "v1",
    "documentation": "https://v3.docs.composio.dev/tools/recallai"
  },
  "auth": {
    "type": "api_key",
    "fields": [
      {
        "name": "region",
        "type": "string",
        "required": true,
        "description": "Recall.ai API region"
      },
      {
        "name": "generic_api_key",
        "type": "password",
        "required": true,
        "description": "Recall.ai API Key"
      }
    ]
  },
  "tools": [
    {
      "name": "create_bot",
      "description": "Create a new meeting bot",
      "endpoint": "/tools/recallai/create_bot",
      "method": "POST",
      "parameters": {
        "meeting_url": {
          "type": "string",
          "required": true,
          "description": "URL of the meeting to join"
        },
        "bot_name": {
          "type": "string",
          "required": false,
          "description": "Name for the bot"
        }
      }
    },
    {
      "name": "delete_bot",
      "description": "Delete a meeting bot",
      "endpoint": "/tools/recallai/delete_bot",
      "method": "DELETE",
      "parameters": {
        "bot_id": {
          "type": "string",
          "required": true,
          "description": "ID of the bot to delete"
        }
      }
    },
    {
      "name": "list_bots",
      "description": "List all meeting bots",
      "endpoint": "/tools/recallai/list_bots",
      "method": "GET",
      "parameters": {
        "limit": {
          "type": "number",
          "required": false,
          "description": "Maximum number of bots to return"
        }
      }
    },
    {
      "name": "list_chat_messages",
      "description": "List chat messages from a meeting",
      "endpoint": "/tools/recallai/list_chat_messages",
      "method": "GET",
      "parameters": {
        "bot_id": {
          "type": "string",
          "required": true,
          "description": "ID of the bot"
        }
      }
    },
    {
      "name": "remove_bot_from_call",
      "description": "Remove bot from an active call",
      "endpoint": "/tools/recallai/remove_bot_from_call",
      "method": "POST",
      "parameters": {
        "bot_id": {
          "type": "string",
          "required": true,
          "description": "ID of the bot to remove"
        }
      }
    },
    {
      "name": "retrieve_bot",
      "description": "Retrieve details of a specific bot",
      "endpoint": "/tools/recallai/retrieve_bot",
      "method": "GET",
      "parameters": {
        "bot_id": {
          "type": "string",
          "required": true,
          "description": "ID of the bot to retrieve"
        }
      }
    },
    {
      "name": "start_recording",
      "description": "Start recording a meeting",
      "endpoint": "/tools/recallai/start_recording",
      "method": "POST",
      "parameters": {
        "bot_id": {
          "type": "string",
          "required": true,
          "description": "ID of the bot to start recording"
        }
      }
    },
    {
      "name": "stop_recording",
      "description": "Stop recording a meeting",
      "endpoint": "/tools/recallai/stop_recording",
      "method": "POST",
      "parameters": {
        "bot_id": {
          "type": "string",
          "required": true,
          "description": "ID of the bot to stop recording"
        }
      }
    }
  ]
}