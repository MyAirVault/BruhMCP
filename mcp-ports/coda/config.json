{
  "name": "coda",
  "displayName": "Coda",
  "description": "Collaborative workspace platform that transforms documents into powerful tools for team productivity and project management",
  "category": "productivity",
  "iconUrl": "https://cdn.jsdelivr.net/npm/simple-icons@v9/icons/coda.svg",
  "port": 49239  ,
  "api": {
    "baseURL": "https://api.composio.dev",
    "version": "v1",
    "documentation": "https://v3.docs.composio.dev/tools/coda"
  },
  "auth": {
    "type": "api_key",
    "fields": [
      {
        "name": "api_key",
        "type": "password",
        "required": true,
        "description": "Coda API Key"
      }
    ]
  },
  "tools": [
    {
      "name": "copy_document",
      "description": "Copy a Coda document",
      "endpoint": "/tools/coda/copy_document",
      "method": "POST",
      "parameters": {
        "document_id": {
          "type": "string",
          "required": true,
          "description": "Document ID to copy"
        },
        "title": {
          "type": "string",
          "required": false,
          "description": "Title for the copied document"
        }
      }
    },
    {
      "name": "list_pack_listings",
      "description": "List available pack listings",
      "endpoint": "/tools/coda/list_pack_listings",
      "method": "GET",
      "parameters": {}
    },
    {
      "name": "search_documents",
      "description": "Search Coda documents",
      "endpoint": "/tools/coda/search_documents",
      "method": "GET",
      "parameters": {
        "query": {
          "type": "string",
          "required": true,
          "description": "Search query"
        }
      }
    },
    {
      "name": "search_table_rows",
      "description": "Search rows in a Coda table",
      "endpoint": "/tools/coda/search_table_rows",
      "method": "GET",
      "parameters": {
        "document_id": {
          "type": "string",
          "required": true,
          "description": "Document ID"
        },
        "table_id": {
          "type": "string",
          "required": true,
          "description": "Table ID"
        },
        "query": {
          "type": "string",
          "required": false,
          "description": "Search query"
        }
      }
    },
    {
      "name": "insert_update_rows",
      "description": "Insert or update rows in a Coda table",
      "endpoint": "/tools/coda/insert_update_rows",
      "method": "POST",
      "parameters": {
        "document_id": {
          "type": "string",
          "required": true,
          "description": "Document ID"
        },
        "table_id": {
          "type": "string",
          "required": true,
          "description": "Table ID"
        },
        "rows": {
          "type": "array",
          "required": true,
          "description": "Array of row data"
        }
      }
    },
    {
      "name": "create_document",
      "description": "Create a new Coda document",
      "endpoint": "/tools/coda/create_document",
      "method": "POST",
      "parameters": {
        "title": {
          "type": "string",
          "required": true,
          "description": "Document title"
        },
        "source_doc": {
          "type": "string",
          "required": false,
          "description": "Source document ID to copy from"
        }
      }
    },
    {
      "name": "delete_document",
      "description": "Delete a Coda document",
      "endpoint": "/tools/coda/delete_document",
      "method": "DELETE",
      "parameters": {
        "document_id": {
          "type": "string",
          "required": true,
          "description": "Document ID to delete"
        }
      }
    },
    {
      "name": "create_page",
      "description": "Create a new page in a document",
      "endpoint": "/tools/coda/create_page",
      "method": "POST",
      "parameters": {
        "document_id": {
          "type": "string",
          "required": true,
          "description": "Document ID"
        },
        "name": {
          "type": "string",
          "required": true,
          "description": "Page name"
        },
        "content": {
          "type": "string",
          "required": false,
          "description": "Page content"
        }
      }
    },
    {
      "name": "manage_permissions",
      "description": "Manage document permissions",
      "endpoint": "/tools/coda/manage_permissions",
      "method": "POST",
      "parameters": {
        "document_id": {
          "type": "string",
          "required": true,
          "description": "Document ID"
        },
        "email": {
          "type": "string",
          "required": true,
          "description": "User email"
        },
        "role": {
          "type": "string",
          "required": true,
          "description": "Permission role (viewer, editor, admin)"
        }
      }
    }
  ]
}