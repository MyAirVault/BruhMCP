{
  "#coming_soon": "Stack Exchange API documentation is not yet available in Composio docs",
  "name": "stackexchange",
  "displayName": "Stack Exchange",
  "description": "Stack Exchange is a network of question-and-answer websites covering various topics in fields such as computer programming, technology, and science",
  "category": "collaboration",
  "iconUrl": "https://cdn.jsdelivr.net/npm/simple-icons@v9/icons/stackexchange.svg",
  "port": 49468  ,
  "api": {
    "baseURL": "https://api.stackexchange.com",
    "version": "2.3",
    "documentation": "https://api.stackexchange.com/docs"
  },
  "auth": {
    "type": "api_key",
    "fields": [
      {
        "name": "api_key",
        "type": "password",
        "required": true,
        "description": "Stack Exchange API Key"
      },
      {
        "name": "access_token",
        "type": "password",
        "required": false,
        "description": "OAuth Access Token for authenticated requests"
      }
    ]
  },
  "tools": [
    {
      "name": "search_questions",
      "description": "Search for questions on Stack Exchange",
      "endpoint": "/search",
      "method": "GET",
      "parameters": {
        "q": {
          "type": "string",
          "required": true,
          "description": "Search query"
        },
        "site": {
          "type": "string",
          "required": true,
          "description": "Stack Exchange site (e.g., stackoverflow, serverfault)",
          "default": "stackoverflow"
        },
        "pagesize": {
          "type": "number",
          "required": false,
          "description": "Number of results per page",
          "default": 30
        }
      }
    },
    {
      "name": "get_question_details",
      "description": "Get detailed information about a question",
      "endpoint": "/questions/{id}",
      "method": "GET",
      "parameters": {
        "id": {
          "type": "string",
          "required": true,
          "description": "Question ID"
        },
        "site": {
          "type": "string",
          "required": true,
          "description": "Stack Exchange site"
        }
      }
    },
    {
      "name": "get_answers",
      "description": "Get answers for a question",
      "endpoint": "/questions/{id}/answers",
      "method": "GET",
      "parameters": {
        "id": {
          "type": "string",
          "required": true,
          "description": "Question ID"
        },
        "site": {
          "type": "string",
          "required": true,
          "description": "Stack Exchange site"
        }
      }
    }
  ]
}