{
  "name": "kibana",
  "displayName": "Kibana",
  "description": "Visualization and analytics platform for Elasticsearch, offering dashboards, data exploration, and monitoring capabilities",
  "category": "analytics",
  "iconUrl": "https://cdn.jsdelivr.net/npm/simple-icons@v9/icons/kibana.svg",
  "port": 49334  ,
  "api": {
    "baseURL": "https://localhost:5601",
    "version": "v1",
    "documentation": "https://v3.docs.composio.dev/tools/kibana"
  },
  "auth": {
    "type": "basic",
    "fields": [
      {
        "name": "base_url",
        "type": "text",
        "required": true,
        "description": "Kibana server URL",
        "default": "https://localhost:5601"
      },
      {
        "name": "username",
        "type": "text",
        "required": true,
        "description": "Kibana username"
      },
      {
        "name": "password",
        "type": "password",
        "required": true,
        "description": "Kibana password"
      }
    ],
    "alternativeAuth": {
      "type": "api_key",
      "fields": [
        {
          "name": "base_url",
          "type": "text",
          "required": true,
          "description": "Kibana server URL",
          "default": "https://localhost:5601"
        },
        {
          "name": "generic_api_key",
          "type": "password",
          "required": true,
          "description": "Kibana API Key"
        }
      ]
    }
  },
  "tools": [
    {
      "name": "delete_action",
      "description": "Delete a specific action",
      "endpoint": "/api/actions/action/{actionId}",
      "method": "DELETE",
      "parameters": {
        "actionId": {
          "type": "string",
          "required": true,
          "description": "Action ID to delete"
        }
      }
    },
    {
      "name": "delete_alerting_rule",
      "description": "Delete an alerting rule",
      "endpoint": "/api/alerting/rule/{ruleId}",
      "method": "DELETE",
      "parameters": {
        "ruleId": {
          "type": "string",
          "required": true,
          "description": "Rule ID to delete"
        }
      }
    },
    {
      "name": "delete_connectors",
      "description": "Delete connectors",
      "endpoint": "/api/actions/connectors/{connectorId}",
      "method": "DELETE",
      "parameters": {
        "connectorId": {
          "type": "string",
          "required": true,
          "description": "Connector ID to delete"
        }
      }
    },
    {
      "name": "delete_fleet_output",
      "description": "Delete Fleet output configuration",
      "endpoint": "/api/fleet/outputs/{outputId}",
      "method": "DELETE",
      "parameters": {
        "outputId": {
          "type": "string",
          "required": true,
          "description": "Output ID to delete"
        }
      }
    },
    {
      "name": "delete_fleet_proxy",
      "description": "Delete Fleet proxy configuration",
      "endpoint": "/api/fleet/proxies/{proxyId}",
      "method": "DELETE",
      "parameters": {
        "proxyId": {
          "type": "string",
          "required": true,
          "description": "Proxy ID to delete"
        }
      }
    },
    {
      "name": "delete_list",
      "description": "Delete a list",
      "endpoint": "/api/lists/{listId}",
      "method": "DELETE",
      "parameters": {
        "listId": {
          "type": "string",
          "required": true,
          "description": "List ID to delete"
        }
      }
    },
    {
      "name": "find_kibana_alerts",
      "description": "Find and search Kibana alerts",
      "endpoint": "/api/alerting/rules/_find",
      "method": "GET",
      "parameters": {
        "search": {
          "type": "string",
          "required": false,
          "description": "Search term for alerts"
        },
        "ruleTypeId": {
          "type": "string",
          "required": false,
          "description": "Filter by rule type ID"
        },
        "consumer": {
          "type": "string",
          "required": false,
          "description": "Filter by consumer"
        },
        "tags": {
          "type": "array",
          "required": false,
          "description": "Filter by tags"
        }
      }
    },
    {
      "name": "get_action_types",
      "description": "Get available action types",
      "endpoint": "/api/actions/connector_types",
      "method": "GET",
      "parameters": {}
    },
    {
      "name": "get_alerting_rules",
      "description": "Get alerting rules",
      "endpoint": "/api/alerting/rules/_find",
      "method": "GET",
      "parameters": {
        "page": {
          "type": "number",
          "required": false,
          "description": "Page number for pagination"
        },
        "perPage": {
          "type": "number",
          "required": false,
          "description": "Items per page"
        }
      }
    },
    {
      "name": "get_cases",
      "description": "Get cases from Kibana",
      "endpoint": "/api/cases",
      "method": "GET",
      "parameters": {
        "page": {
          "type": "number",
          "required": false,
          "description": "Page number for pagination"
        },
        "perPage": {
          "type": "number",
          "required": false,
          "description": "Items per page"
        },
        "status": {
          "type": "string",
          "required": false,
          "description": "Filter by case status"
        }
      }
    },
    {
      "name": "get_connectors",
      "description": "Get available connectors",
      "endpoint": "/api/actions/connectors",
      "method": "GET",
      "parameters": {}
    },
    {
      "name": "get_data_views",
      "description": "Get data views (index patterns)",
      "endpoint": "/api/data_views",
      "method": "GET",
      "parameters": {}
    },
    {
      "name": "find_detection_engine_rules",
      "description": "Find detection engine rules",
      "endpoint": "/api/detection_engine/rules/_find",
      "method": "GET",
      "parameters": {
        "page": {
          "type": "number",
          "required": false,
          "description": "Page number for pagination"
        },
        "per_page": {
          "type": "number",
          "required": false,
          "description": "Items per page"
        },
        "sort_field": {
          "type": "string",
          "required": false,
          "description": "Field to sort by"
        },
        "sort_order": {
          "type": "string",
          "required": false,
          "description": "Sort order (asc/desc)"
        }
      }
    }
  ]
}