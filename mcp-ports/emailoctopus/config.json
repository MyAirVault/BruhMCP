{
  "name": "emailoctopus",
  "displayName": "EmailOctopus",
  "description": "Affordable and intuitive email marketing platform offering solutions for individuals and businesses to manage email campaigns and lists",
  "category": "email",
  "iconUrl": "https://cdn.jsdelivr.net/npm/simple-icons@v9/icons/email.svg",
  "port": 49270  ,
  "api": {
    "baseURL": "https://emailoctopus.com/api/1.6",
    "version": "1.6",
    "documentation": "https://v3.docs.composio.dev/tools/emailoctopus"
  },
  "auth": {
    "type": "api_key",
    "fields": [
      {
        "name": "api_key",
        "type": "password",
        "required": true,
        "description": "EmailOctopus API Key"
      }
    ]
  },
  "tools": [
    {
      "name": "create_contact",
      "description": "Create a new contact in a list",
      "endpoint": "/lists/{list_id}/contacts",
      "method": "POST",
      "parameters": {
        "list_id": {
          "type": "string",
          "required": true,
          "description": "ID of the list to add contact to"
        },
        "email_address": {
          "type": "string",
          "required": true,
          "description": "Email address of the contact"
        },
        "fields": {
          "type": "object",
          "required": false,
          "description": "Additional fields for the contact"
        },
        "status": {
          "type": "string",
          "required": false,
          "description": "Status of the contact (SUBSCRIBED, UNSUBSCRIBED)"
        },
        "tags": {
          "type": "array",
          "required": false,
          "description": "Tags to assign to the contact"
        }
      }
    },
    {
      "name": "create_list",
      "description": "Create a new email list",
      "endpoint": "/lists",
      "method": "POST",
      "parameters": {
        "name": {
          "type": "string",
          "required": true,
          "description": "Name of the email list"
        }
      }
    },
    {
      "name": "delete_contact",
      "description": "Delete a contact from a list",
      "endpoint": "/lists/{list_id}/contacts/{contact_id}",
      "method": "DELETE",
      "parameters": {
        "list_id": {
          "type": "string",
          "required": true,
          "description": "ID of the list containing the contact"
        },
        "contact_id": {
          "type": "string",
          "required": true,
          "description": "ID of the contact to delete"
        }
      }
    },
    {
      "name": "delete_list",
      "description": "Delete an email list",
      "endpoint": "/lists/{list_id}",
      "method": "DELETE",
      "parameters": {
        "list_id": {
          "type": "string",
          "required": true,
          "description": "ID of the list to delete"
        }
      }
    },
    {
      "name": "get_all_lists",
      "description": "Retrieve all email lists",
      "endpoint": "/lists",
      "method": "GET",
      "parameters": {
        "limit": {
          "type": "number",
          "required": false,
          "description": "Number of lists to return",
          "default": 100
        },
        "page": {
          "type": "number",
          "required": false,
          "description": "Page number for pagination",
          "default": 1
        }
      }
    },
    {
      "name": "get_recent_campaigns",
      "description": "Get recent email campaigns",
      "endpoint": "/campaigns",
      "method": "GET",
      "parameters": {
        "limit": {
          "type": "number",
          "required": false,
          "description": "Number of campaigns to return",
          "default": 100
        },
        "page": {
          "type": "number",
          "required": false,
          "description": "Page number for pagination",
          "default": 1
        }
      }
    },
    {
      "name": "unsubscribe_contact",
      "description": "Unsubscribe a contact from a list",
      "endpoint": "/lists/{list_id}/contacts/{contact_id}",
      "method": "PUT",
      "parameters": {
        "list_id": {
          "type": "string",
          "required": true,
          "description": "ID of the list containing the contact"
        },
        "contact_id": {
          "type": "string",
          "required": true,
          "description": "ID of the contact to unsubscribe"
        },
        "status": {
          "type": "string",
          "required": true,
          "description": "Status to set (UNSUBSCRIBED)",
          "default": "UNSUBSCRIBED"
        }
      }
    }
  ]
}