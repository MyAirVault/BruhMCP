{
  "name": "googlemeet",
  "displayName": "Google Meet",
  "description": "Google Meet is a secure video conferencing platform that integrates with Google Workspace, facilitating remote meetings, screen sharing, and chat",
  "category": "collaboration",
  "iconUrl": "https://cdn.jsdelivr.net/npm/simple-icons@v9/icons/googlemeet.svg",
  "port": 49305  ,
  "api": {
    "baseURL": "https://meet.googleapis.com/v2",
    "version": "v2",
    "documentation": "https://developers.google.com/meet/api"
  },
  "auth": {
    "type": "oauth2",
    "fields": [
      {
        "name": "client_id",
        "type": "text",
        "required": true,
        "description": "Google OAuth Client ID"
      },
      {
        "name": "client_secret",
        "type": "password",
        "required": true,
        "description": "Google OAuth Client Secret"
      }
    ],
    "oauth": {
      "redirect_uri": "https://backend.composio.dev/api/v1/auth-apps/add",
      "scopes": ["https://www.googleapis.com/auth/meetings.space.created", "https://www.googleapis.com/auth/meetings.space.readonly"],
      "authorization_url": "https://accounts.google.com/o/oauth2/auth",
      "token_url": "https://oauth2.googleapis.com/token"
    }
  },
  "tools": [
    {
      "name": "get_conference_record",
      "description": "Get conference record details",
      "endpoint": "/conferenceRecords/{conference_record_id}",
      "method": "GET",
      "parameters": {
        "conference_record_id": {
          "type": "string",
          "required": true,
          "description": "Conference record ID to retrieve"
        }
      }
    },
    {
      "name": "get_participant_session",
      "description": "Get participant session details",
      "endpoint": "/conferenceRecords/{conference_record_id}/participants/{participant_id}/participantSessions/{session_id}",
      "method": "GET",
      "parameters": {
        "conference_record_id": {
          "type": "string",
          "required": true,
          "description": "Conference record ID"
        },
        "participant_id": {
          "type": "string",
          "required": true,
          "description": "Participant ID"
        },
        "session_id": {
          "type": "string",
          "required": true,
          "description": "Session ID"
        }
      }
    },
    {
      "name": "list_conference_records",
      "description": "List conference records",
      "endpoint": "/conferenceRecords",
      "method": "GET",
      "parameters": {
        "pageSize": {
          "type": "number",
          "required": false,
          "description": "Maximum number of records to return",
          "default": 100
        },
        "pageToken": {
          "type": "string",
          "required": false,
          "description": "Page token for pagination"
        }
      }
    },
    {
      "name": "list_participant_sessions",
      "description": "List participant sessions",
      "endpoint": "/conferenceRecords/{conference_record_id}/participants/{participant_id}/participantSessions",
      "method": "GET",
      "parameters": {
        "conference_record_id": {
          "type": "string",
          "required": true,
          "description": "Conference record ID"
        },
        "participant_id": {
          "type": "string",
          "required": true,
          "description": "Participant ID"
        },
        "pageSize": {
          "type": "number",
          "required": false,
          "description": "Maximum number of sessions to return",
          "default": 100
        }
      }
    },
    {
      "name": "update_space",
      "description": "Update Google Meet space",
      "endpoint": "/spaces/{space_id}",
      "method": "PATCH",
      "parameters": {
        "space_id": {
          "type": "string",
          "required": true,
          "description": "Space ID to update"
        },
        "displayName": {
          "type": "string",
          "required": false,
          "description": "Display name for the space"
        },
        "config": {
          "type": "object",
          "required": false,
          "description": "Space configuration settings"
        }
      }
    },
    {
      "name": "create_meet",
      "description": "Create a Meet",
      "endpoint": "/spaces",
      "method": "POST",
      "parameters": {
        "displayName": {
          "type": "string",
          "required": false,
          "description": "Display name for the meeting"
        },
        "config": {
          "type": "object",
          "required": false,
          "description": "Meeting configuration"
        }
      }
    },
    {
      "name": "get_meet_details",
      "description": "Get Meet details",
      "endpoint": "/spaces/{space_id}",
      "method": "GET",
      "parameters": {
        "space_id": {
          "type": "string",
          "required": true,
          "description": "Space ID to get details for"
        }
      }
    },
    {
      "name": "get_recordings",
      "description": "Get recordings by conference record ID",
      "endpoint": "/conferenceRecords/{conference_record_id}/recordings",
      "method": "GET",
      "parameters": {
        "conference_record_id": {
          "type": "string",
          "required": true,
          "description": "Conference record ID to get recordings for"
        },
        "pageSize": {
          "type": "number",
          "required": false,
          "description": "Maximum number of recordings to return",
          "default": 100
        }
      }
    },
    {
      "name": "get_transcripts",
      "description": "Get transcripts by conference record ID",
      "endpoint": "/conferenceRecords/{conference_record_id}/transcripts",
      "method": "GET",
      "parameters": {
        "conference_record_id": {
          "type": "string",
          "required": true,
          "description": "Conference record ID to get transcripts for"
        },
        "pageSize": {
          "type": "number",
          "required": false,
          "description": "Maximum number of transcripts to return",
          "default": 100
        }
      }
    }
  ]
}