{
  "name": "ngrok",
  "displayName": "Ngrok",
  "description": "Ngrok creates secure tunnels to locally hosted applications, enabling developers to share and test webhooks or services",
  "category": "developer",
  "iconUrl": "https://cdn.jsdelivr.net/npm/simple-icons@v9/icons/ngrok.svg",
  "port": 49390  ,
  "api": {
    "baseURL": "https://api.ngrok.com",
    "version": "v2",
    "documentation": "https://ngrok.com/docs/api"
  },
  "auth": {
    "type": "api_key",
    "fields": [
      {
        "name": "api_key",
        "type": "password",
        "required": true,
        "description": "Ngrok API Key"
      }
    ]
  },
  "tools": [
    {
      "name": "create_api_key",
      "description": "Creates a new api key for authenticating with the ngrok api",
      "endpoint": "/api_keys",
      "method": "POST",
      "parameters": {
        "description": {
          "type": "string",
          "required": false,
          "description": "Description for the API key"
        },
        "metadata": {
          "type": "string",
          "required": false,
          "description": "Metadata for the API key"
        }
      }
    },
    {
      "name": "delete_api_key",
      "description": "Delete an api key by its id",
      "endpoint": "/api_keys/{id}",
      "method": "DELETE",
      "parameters": {
        "id": {
          "type": "string",
          "required": true,
          "description": "API Key ID"
        }
      }
    },
    {
      "name": "list_api_keys",
      "description": "Lists all api keys owned by the user",
      "endpoint": "/api_keys",
      "method": "GET",
      "parameters": {
        "before_id": {
          "type": "string",
          "required": false,
          "description": "Pagination cursor"
        },
        "limit": {
          "type": "string",
          "required": false,
          "description": "Maximum number of keys to retrieve"
        }
      }
    },
    {
      "name": "list_endpoints",
      "description": "List all active endpoints on the ngrok account",
      "endpoint": "/endpoints",
      "method": "GET",
      "parameters": {
        "before_id": {
          "type": "string",
          "required": false,
          "description": "Pagination cursor"
        },
        "limit": {
          "type": "string",
          "required": false,
          "description": "Maximum number of endpoints to retrieve"
        }
      }
    },
    {
      "name": "list_tunnels",
      "description": "List all active tunnels in the ngrok account",
      "endpoint": "/tunnels",
      "method": "GET",
      "parameters": {
        "before_id": {
          "type": "string",
          "required": false,
          "description": "Pagination cursor"
        },
        "limit": {
          "type": "string",
          "required": false,
          "description": "Maximum number of tunnels to retrieve"
        }
      }
    }
  ]
}