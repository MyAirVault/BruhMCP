{
  "name": "quickbooks",
  "displayName": "QuickBooks",
  "description": "QuickBooks is a cloud-based accounting software that helps you manage your finances, track your income and expenses",
  "category": "finance",
  "iconUrl": "https://cdn.jsdelivr.net/npm/simple-icons@v9/icons/quickbooks.svg",
  "port": 49418  ,
  "api": {
    "baseURL": "https://api.composio.dev",
    "version": "v1",
    "documentation": "https://v3.docs.composio.dev/tools/quickbooks"
  },
  "auth": {
    "type": "oauth2",
    "fields": [
      {
        "name": "client_id",
        "type": "string",
        "required": true,
        "description": "QuickBooks OAuth2 Client ID"
      },
      {
        "name": "client_secret",
        "type": "password",
        "required": true,
        "description": "QuickBooks OAuth2 Client Secret"
      },
      {
        "name": "oauth_redirect_uri",
        "type": "string",
        "required": false,
        "description": "OAuth2 Redirect URI",
        "default": "https://backend.composio.dev/api/v1/auth-apps/add"
      }
    ]
  },
  "tools": [
    {
      "name": "create_account",
      "description": "Create a new account in QuickBooks",
      "endpoint": "/tools/quickbooks/create_account",
      "method": "POST",
      "parameters": {
        "name": {
          "type": "string",
          "required": true,
          "description": "Account name"
        },
        "account_type": {
          "type": "string",
          "required": true,
          "description": "Type of account"
        }
      }
    },
    {
      "name": "create_customer",
      "description": "Create a new customer in QuickBooks",
      "endpoint": "/tools/quickbooks/create_customer",
      "method": "POST",
      "parameters": {
        "name": {
          "type": "string",
          "required": true,
          "description": "Customer name"
        },
        "email": {
          "type": "string",
          "required": false,
          "description": "Customer email address"
        }
      }
    },
    {
      "name": "create_employee",
      "description": "Create a new employee in QuickBooks",
      "endpoint": "/tools/quickbooks/create_employee",
      "method": "POST",
      "parameters": {
        "name": {
          "type": "string",
          "required": true,
          "description": "Employee name"
        }
      }
    },
    {
      "name": "create_vendor",
      "description": "Create a new vendor in QuickBooks",
      "endpoint": "/tools/quickbooks/create_vendor",
      "method": "POST",
      "parameters": {
        "name": {
          "type": "string",
          "required": true,
          "description": "Vendor name"
        }
      }
    },
    {
      "name": "query_account",
      "description": "Query accounts in QuickBooks",
      "endpoint": "/tools/quickbooks/query_account",
      "method": "GET",
      "parameters": {
        "account_type": {
          "type": "string",
          "required": false,
          "description": "Filter by account type"
        }
      }
    },
    {
      "name": "read_customer",
      "description": "Read customer details",
      "endpoint": "/tools/quickbooks/read_customer",
      "method": "GET",
      "parameters": {
        "customer_id": {
          "type": "string",
          "required": true,
          "description": "Customer ID to retrieve"
        }
      }
    },
    {
      "name": "customer_balance_report",
      "description": "Generate customer balance report",
      "endpoint": "/tools/quickbooks/customer_balance_report",
      "method": "GET",
      "parameters": {
        "start_date": {
          "type": "string",
          "required": false,
          "description": "Report start date"
        },
        "end_date": {
          "type": "string",
          "required": false,
          "description": "Report end date"
        }
      }
    },
    {
      "name": "vendor_balance_report",
      "description": "Generate vendor balance report",
      "endpoint": "/tools/quickbooks/vendor_balance_report",
      "method": "GET",
      "parameters": {
        "start_date": {
          "type": "string",
          "required": false,
          "description": "Report start date"
        },
        "end_date": {
          "type": "string",
          "required": false,
          "description": "Report end date"
        }
      }
    }
  ]
}